â‘£ Pythonã§å‡¦ç†ï¼ˆãƒ­ãƒ¼ã‚«ãƒ«ï¼‰
import boto3

textract = boto3.client("textract")

res = textract.get_document_text_detection(
    JobId="xxxxxxxxx"
)

lines = [
    b["Text"]
    for b in res["Blocks"]
    if b["BlockType"] == "LINE"
]

full_text = "\n".join(lines)
print(full_text)


